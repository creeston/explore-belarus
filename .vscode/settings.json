{
    "[svelte]": {
        "editor.defaultFormatter": "svelte.svelte-vscode",
        "editor.indentSize": "tabSize",
        "editor.formatOnSave": true,
        "editor.insertSpaces": true,
        "editor.detectIndentation": false,
        "editor.tabSize": 4
    },
    "[javascript]": {
        "editor.tabSize": 4
    },
    "[typescript]": {
        "editor.tabSize": 4
    },
    "prettier.tabWidth": 4,
    "prettier.useTabs": false
}